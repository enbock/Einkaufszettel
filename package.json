{
  "name": "einkaufszettel",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@enbock/string-helper-ts": "^1",
    "@enbock/ts-jsx": "^0.1.0-dev14",
    "@testing-library/dom": "^8",
    "@testing-library/jest-dom": "^5",
    "@testing-library/user-event": "^14",
    "@types/jest": "^27",
    "@types/lodash.get": "^4",
    "@types/node": "^16",
    "@types/uuid": "^8",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28",
    "jest-environment-jsdom": "^28.0.1",
    "jest-mock-extended": "^2",
    "lodash.get": "^4",
    "style-loader": "^3.3.1",
    "svg-inline-loader": "^0.8.2",
    "ts-jest": "^28.0.0-next.1",
    "typescript": "^4",
    "uuid": "^8",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0",
    "workbox-core": "^6",
    "workbox-expiration": "^6",
    "workbox-precaching": "^6",
    "workbox-routing": "^6",
    "workbox-strategies": "^6"
  },
  "scripts": {
    "start": "webpack serve --open --mode development --no-open --no-live-reload --no-hot --stats-error-details",
    "build": "webpack --mode production",
    "test": "jest"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "!src/index.ts",
      "!src/*.js",
      "!src/**/*.json",
      "!src/**/*.svg"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "verbose": true,
    "collectCoverage": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest/legacy",
      "^.+\\.jsx?$": "ts-jest/legacy",
      "^.+\\.svg$": "<rootDir>/testTransform.js",
      "^.+\\.css$": "<rootDir>/testTransform.js"
    },
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "testRegex": "\\.test\\.tsx?$",
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "node_modules/?!(uuid|@enbock/string-helper-ts)"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ]
  }
}
