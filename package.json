{
  "name": "einkaufszettel",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@enbock/string-helper-ts": "^1",
    "@testing-library/jest-dom": "^5",
    "@testing-library/react": "^12",
    "@testing-library/user-event": "^13",
    "lodash.get": "^4",
    "react": "^17",
    "react-dom": "^17",
    "react-scripts": "^4",
    "typescript": "^4",
    "uuid": "^8",
    "web-vitals": "^0",
    "jest-mock-extended": "^2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "test-build": "yarn build && npx serve -s build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "eqeqeq": [
        0
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^27",
    "@types/lodash.get": "^4",
    "@types/node": "^16",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "@types/uuid": "^8"
  },
  "jest": {
    "collectCoverageFrom": [
      "!src/index.ts",
      "!src/*.js",
      "!src/**/*.json",
      "!src/**/*.svg"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!(@enbock)/)"
    ]
  }
}
